                                            <p class="d-inline"><i>
                                                <% product.weights.forEach(weight => { %>
                                                    <i><%= weight.value %> kg /</i>
                                                <% }); %>
                                            </i></p>
                                        </div>
                                        <div>
                                            <label class="text-dark m-3">Dimentions : </label>
